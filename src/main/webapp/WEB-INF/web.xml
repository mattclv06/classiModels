<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
    <listener-class>classiModels.BDD.config.InitialisationDAOFactory</listener-class>
</listener>
  <servlet>
  	<servlet-name>ProductLineServlet</servlet-name>
  	<display-name>ProductLineServlet</display-name>
  	<description></description>
  	<servlet-class>classiModels.Servlet.ProductLineServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Panier</servlet-name>
  	<display-name>Panier</display-name>
  	<description></description>
  	<servlet-class>classiModels.Servlet.PanierServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SupPanier</servlet-name>
  	<display-name>SupPanier</display-name>
  	<description></description>
  	<servlet-class>classiModels.Servlet.SupPanier</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ProductLineServlet</servlet-name>
  	<url-pattern>/ProductLine</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Panier</servlet-name>
  	<url-pattern>/Panier</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SupPanier</servlet-name>
  	<url-pattern>/SupPanier</url-pattern>
  </servlet-mapping>
  <session-config>
  	<session-timeout>1</session-timeout>
  </session-config>
</web-app>
